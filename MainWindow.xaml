<basic:MyWindow  xmlns:basic="clr-namespace:Velacro.UIElements.Basic;assembly=Velacro" 
                 x:Class="Hotelin_Desktop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hotelin_Desktop"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Frame Name="mainFrame" NavigationUIVisibility="Hidden" Margin="78,0,0,0" />
        <Button Name="login_btn" Content="Login" HorizontalAlignment="Left" Margin="3,10,0,0" VerticalAlignment="Top" Width="75" Click="login_btn_onCLick" />
        <Button Name="register_btn" Content="Register" HorizontalAlignment="Left" Margin="3,35,0,0" VerticalAlignment="Top" Width="75" Click="register_btn_onCLick" />
    </Grid>
</basic:MyWindow>
